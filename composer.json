{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "5.2.*",
    "laravelcollective/html": "5.*",
    "filp/whoops": "~1.0",
    "fzaninotto/faker": "dev-master",
    "guzzlehttp/guzzle": "~5.0",
    "barryvdh/laravel-ide-helper": "^2.0",
    "erusev/parsedown": "1.*",
    "doctrine/dbal": "~2.3",
    "intervention/image": "^2.3",
    "kodicms/module-loader": "3.*",
    "kodicms/laravel-api": "^1.0",
    "kodicms/laravel-assets": "0.*",
    "kodicms/laravel-navigation": "2.*",
    "kodicms/users": "0.*",
    "kodicms/dashboard": "0.*",
    "kodicms/widgets": "0.*",
    "kodicms/pages": "0.*",
    "kodicms/datasource": "0.*",
    "kodicms/filemanager": "0.*",
    "kodicms/plugins": "0.*",
    "kodicms/userguide": "0.*"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1",
    "symfony/dom-crawler": "~3.0",
    "symfony/css-selector": "~3.0"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/",
      "KodiCMS\\": "modules/"
    },
    "files": [
      "modules/CMS/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "pre-update-cmd": [
      "php artisan clear-compiled"
    ],
    "post-update-cmd": [
      "php artisan ide-helper:generate",
      "php artisan optimize"
    ],
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$vendor}/{$name}": ["type:kodicms-plugin"]
    }
  }
}
